{
  "name": "streema-api",
  "version": "0.0.1",
  "description": "Unofficial Streema API",
  "author": "Andrew Gabriel",
  "license": "MIT",
  "types": "dist/cjs/streemaApi.d.ts",
  "main": "dist/cjs/streemaApi.js",
  "module": "dist/esm/streemaApi.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/streemaApi.d.ts",
        "default": "./dist/esm/streemaApi.js"
      },
      "require": {
        "types": "./dist/cjs/streemaApi.d.ts",
        "default": "./dist/cjs/streemaApi.js"
      }
    },
    "./models": {
      "import": {
        "types": "./dist/esm/models.d.ts",
        "default": "./dist/esm/models.js"
      },
      "require": {
        "types": "./dist/cjs/models.d.ts",
        "default": "./dist/cjs/models.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "npm run build && jest",
    "test:watch": "npm run build && jest --watch",
    "test:watchAll": "npm run build && jest --watchAll",
    "clean": "rm -rf dist",
    "build": "rm -fr dist/* && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.json && echo '{\"type\":\"module\"}' > dist/esm/package.json",
    "build:cjs": "tsc -p tsconfig-cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "rebuild": "npm run clean && npm run build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "keywords": [
    "streema",
    "streema-api"
  ],
  "dependencies": {
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "follow-redirects": "^1.15.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.35",
    "@types/follow-redirects": "^1.14.4",
    "@types/jest": "^29.5.12",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
